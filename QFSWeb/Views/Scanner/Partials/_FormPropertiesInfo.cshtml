@model List<InfoPathServices.Property>

<div id="formProperties">
    <div class="tableDiv">
        <table class="xsnInfo bordered">
            <tr>
                <th>Name</th>
                <th>Value</th>
            </tr>
            @foreach (var p in Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>@p.Value</td>
                </tr>
            }
        </table>
    </div>
</div>

